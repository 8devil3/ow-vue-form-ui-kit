{
    "name": "ow-vue-form-ui-kit",
    "author": "OrangeWeb",
    "version": "1.0.12",
    "description": "Form UI kit Vue 3, based on Tailwind CSS.",
    "homepage": "https://www.tailwindtoolset.app/orangevue",
    "keywords": [
        "form-ui",
        "vue",
        "form",
        "ui"
    ],
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not dead"
    ],
    "files": [
        "dist"
    ],
    "private": false,
    "main": "./dist/ow-vue-form-ui-kit.umd.js",
    "module": "./dist/ow-vue-form-ui-kit.es.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/ow-vue-form-ui-kit.es.js",
            "require": "./dist/ow-vue-form-ui-kit.umd.js"
        }
    },
    "engines": {
        "node": ">=14"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/8devil3/ow-vue-form-ui-kit.git"
    },
    "bugs": {
        "url": "https://github.com/8devil3/ow-vue-form-ui-kit/issues"
    },
    "scripts": {
        "dev": "vite",
        "build": "vite build && vue-tsc --emitDeclarationOnly",
        "preview": "vite preview",
        "build-only": "vite build",
        "type-check": "vue-tsc --noEmit"
    },
    "dependencies": {
        "vue": "^3.2.47"
    },
    "devDependencies": {
        "@tailwindcss/forms": "^0.5.3",
        "@types/node": "^18.14.2",
        "@vitejs/plugin-vue": "^4.0.0",
        "@vue/tsconfig": "^0.1.3",
        "autoprefixer": "^10.4.13",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.21",
        "tailwindcss": "^3.2.7",
        "typescript": "~4.8.4",
        "vite": "^4.1.4",
        "vue-tsc": "^1.2.0"
    }
}
